    "dev:base": "cd dmp-base && pnpm dev",
    "dev:cli": "cd dmp-cli-vue3 && pnpm dev",
    "dev:v2": "cd dmp-v2 && pnpm dev",
    "dev:vite3": "cd dmp-vite-vue3 && pnpm dev",
